class Solution {
    public String interpret(String c) {
        int n=c.length();
        StringBuilder s=new StringBuilder(n);
        for(int i=0;i<n;i++){
            if(c.charAt(i)=='G') s.append('G');
            if(c.charAt(i)=='('){
                if(c.charAt(i+1)==')'){
                s.append('o');
                i++;
                }else{
                    s.append("al");
                    i=i+3;
                }
            }
        }return s.toString();
    }
}
