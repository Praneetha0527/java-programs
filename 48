class Solution {
    public void rotate(int[][] n) {
        for(int i=0;i<n.length;i++){
            for(int j=i+1;j<n.length;j++){
                int temp=n[i][j];
                n[i][j] = n[j][i];
                n[j][i] = temp;
            }
        }
        for(int i=0;i<n.length;i++){
            for(int j=0;j<n.length/2;j++){
                int temp = n[i][j];
                n[i][j] = n[i][n.length-1-j];
                n[i][n.length-1-j]=temp;
            }
        }
    }
}
