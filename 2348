class Solution {
    public long zeroFilledSubarray(int[] nums) {
        int n=nums.length;
        long c=0,a=0;
        for(int i=0;i<n;i++){
            if(nums[i]==0){
                a++;
            }else{
                c+=(a*(a+1)/2);
                a=0;
            }
        }
        c+=(a*(a+1)/2);
        return c;
    }
}
